{
    "name": "becommerce/laravel-postman",
    "description": "Generate a Postman collection automatically from your Laravel API",
    "keywords": [
        "laravel",
        "postman",
        "api",
        "collection",
        "generate",
        "andreaselia"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Elia",
            "email": "andreaselia@live.co.uk"
        },
        {
            "name": "Ronald Appleton",
            "email": "ron.appleton@becommerce.shop"
        },
        {
            "name": "Paul Dawson",
            "email": "paul.dawson@becommerce.shop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Becommerce\\PostmanGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Becommerce\\PostmanGenerator\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/config": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/routing": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Becommerce\\PostmanGenerator\\PostmanGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^7.3",
        "stematic/testing": "dev-develop"
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
